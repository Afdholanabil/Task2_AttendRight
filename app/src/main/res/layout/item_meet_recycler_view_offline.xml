<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="130dp"
    android:background="@color/white">

    <TextView
        android:id="@+id/txt_title_member_meet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="21dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="@string/meeting_for_world_war_3"
        android:textColor="@color/fontgray2"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintStatus"
        android:layout_width="68dp"
        android:layout_height="18dp"
        android:layout_marginEnd="25dp"
        android:background="@drawable/rounded_status_offline"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="@id/txt_title_member_meet"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/txt_title_member_meet">

        <ImageView
            android:id="@+id/ic_status_online"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:src="@drawable/offline"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="ContentDescription,MissingConstraints" />

        <TextView
            android:id="@+id/txt_title_status_meet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_medium"
            android:text="@string/offline"
            android:textColor="@color/red"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="@id/ic_status_online"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/ic_status_online"
            app:layout_constraintTop_toTopOf="@id/ic_status_online"
            tools:ignore="MissingConstraints" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/ic_link_online"
        android:layout_width="24dp"
        android:layout_height="22dp"
        android:layout_marginTop="12dp"
        android:layout_marginStart="25dp"
        android:src="@drawable/location"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/txt_title_member_meet"
        tools:ignore="ContentDescription,MissingConstraints" />

    <TextView
        android:id="@+id/txt_title_link_meet"
        android:layout_width="185dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:layout_marginBottom="21dp"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/meet_google_com_eer_iuyi_opk"
        android:textColor="@color/gray3"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@id/ic_link_online"
        app:layout_constraintTop_toTopOf="@id/ic_link_online"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/txt_title_time_meet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="25dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="@string/_15_20_pm"
        android:textColor="@color/gray3"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/txt_title_link_meet"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>