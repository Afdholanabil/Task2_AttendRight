<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="34dp"
    android:backgroundTint="@android:color/background_light"
    tools:context=".presentation.ui.fragments.ProfileFragment">

    <ImageView
        android:id="@+id/iv_profile_profile"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/image_26"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <TextView
        android:id="@+id/tv_nama_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/ginanjar_putra"
        android:textColor="@color/gray600"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@id/tv_role_profile_profile"
        app:layout_constraintStart_toEndOf="@id/iv_profile_profile"
        app:layout_constraintTop_toTopOf="@id/iv_profile_profile" />

    <TextView
        android:id="@+id/tv_role_profile_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/_0098776_ui_ux_developer"
        android:textSize="14sp"
        android:textColor="@color/gray400"
        android:fontFamily="@font/poppins_regular"
        app:layout_constraintStart_toEndOf="@id/iv_profile_profile"
        app:layout_constraintTop_toBottomOf="@id/tv_nama_profile"
        app:layout_constraintBottom_toBottomOf="@id/iv_profile_profile"
        android:layout_marginStart="20dp"/>

    <ImageView
        android:id="@+id/iv_my_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/icon_my_profile"
        android:src="@drawable/user_edit"
        app:layout_constraintTop_toBottomOf="@id/iv_profile_profile"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="50dp"/>
    <TextView
        android:id="@+id/tv_my_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/my_profile"
        android:textSize="16sp"
        android:textColor="@color/gray600"
        android:fontFamily="@font/poppins_semibold"
        app:layout_constraintStart_toEndOf="@id/iv_my_profile"
        app:layout_constraintTop_toTopOf="@id/iv_my_profile"
        app:layout_constraintBottom_toBottomOf="@id/iv_my_profile"
        android:layout_marginStart="20dp"/>

    <ImageView
        android:id="@+id/iv_next_my_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/icon_see_detail"
        android:src="@drawable/vector_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/iv_my_profile"
        app:layout_constraintBottom_toBottomOf="@id/iv_my_profile"/>

    <ImageView
        android:id="@+id/iv_job_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/icon_my_profile"
        android:src="@drawable/vector"
        app:layout_constraintTop_toBottomOf="@id/iv_my_profile"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="40dp"/>
    <TextView
        android:id="@+id/tv_job_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/job_information"
        android:textSize="16sp"
        android:textColor="@color/gray600"
        android:fontFamily="@font/poppins_semibold"
        app:layout_constraintStart_toEndOf="@id/iv_job_info"
        app:layout_constraintTop_toTopOf="@id/iv_job_info"
        app:layout_constraintBottom_toBottomOf="@id/iv_job_info"
        android:layout_marginStart="20dp"/>

    <ImageView
        android:id="@+id/iv_next_job_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/icon_see_detail"
        android:src="@drawable/vector_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/iv_job_info"
        app:layout_constraintBottom_toBottomOf="@id/iv_job_info"/>

    <ImageView
        android:id="@+id/iv_poin_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/icon_my_profile"
        android:src="@drawable/coin"
        app:layout_constraintTop_toBottomOf="@id/iv_job_info"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="40dp"/>
    <TextView
        android:id="@+id/tv_poin_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/poin"
        android:textSize="16sp"
        android:textColor="@color/gray600"
        android:fontFamily="@font/poppins_semibold"
        app:layout_constraintStart_toEndOf="@id/iv_poin_profile"
        app:layout_constraintTop_toTopOf="@id/iv_poin_profile"
        app:layout_constraintBottom_toBottomOf="@id/iv_poin_profile"
        android:layout_marginStart="20dp"/>

    <ImageView
        android:id="@+id/iv_next_poin_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/icon_see_detail"
        android:src="@drawable/vector_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/iv_poin_profile"
        app:layout_constraintBottom_toBottomOf="@id/iv_poin_profile"/>

    <ImageView
        android:id="@+id/iv_faq_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/icon_my_profile"
        android:src="@drawable/group_6"
        app:layout_constraintTop_toBottomOf="@id/iv_poin_profile"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="40dp"/>
    <TextView
        android:id="@+id/tv_faq_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/faq"
        android:textSize="16sp"
        android:textColor="@color/gray600"
        android:fontFamily="@font/poppins_semibold"
        app:layout_constraintStart_toEndOf="@id/iv_faq_profile"
        app:layout_constraintTop_toTopOf="@id/iv_faq_profile"
        app:layout_constraintBottom_toBottomOf="@id/iv_faq_profile"
        android:layout_marginStart="20dp"/>

    <ImageView
        android:id="@+id/iv_next_faq_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/icon_see_detail"
        android:src="@drawable/vector_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/iv_faq_profile"
        app:layout_constraintBottom_toBottomOf="@id/iv_faq_profile"/>

    <ImageView
        android:id="@+id/iv_setting_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/icon_my_profile"
        android:src="@drawable/setting_2"
        app:layout_constraintTop_toBottomOf="@id/iv_faq_profile"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="40dp"/>
    <TextView
        android:id="@+id/tv_setting_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/setting"
        android:textSize="16sp"
        android:textColor="@color/gray600"
        android:fontFamily="@font/poppins_semibold"
        app:layout_constraintStart_toEndOf="@id/iv_setting_profile"
        app:layout_constraintTop_toTopOf="@id/iv_setting_profile"
        app:layout_constraintBottom_toBottomOf="@id/iv_setting_profile"
        android:layout_marginStart="20dp"/>

    <ImageView
        android:id="@+id/iv_next_setting_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/icon_see_detail"
        android:src="@drawable/vector_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/iv_setting_profile"
        app:layout_constraintBottom_toBottomOf="@id/iv_setting_profile"/>

    <ImageView
        android:id="@+id/iv_about_app_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/icon_my_profile"
        android:src="@drawable/message_question"
        app:layout_constraintTop_toBottomOf="@id/iv_setting_profile"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="40dp"/>
    <TextView
        android:id="@+id/tv_about_app_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/about_app"
        android:textSize="16sp"
        android:textColor="@color/gray600"
        android:fontFamily="@font/poppins_semibold"
        app:layout_constraintStart_toEndOf="@id/iv_about_app_profile"
        app:layout_constraintTop_toTopOf="@id/iv_about_app_profile"
        app:layout_constraintBottom_toBottomOf="@id/iv_about_app_profile"
        android:layout_marginStart="20dp"/>

    <ImageView
        android:id="@+id/iv_next_about_app_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/icon_see_detail"
        android:src="@drawable/vector_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/iv_about_app_profile"
        app:layout_constraintBottom_toBottomOf="@id/iv_about_app_profile"/>

    <ImageView
        android:id="@+id/iv_logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/icon_my_profile"
        android:src="@drawable/logout_red"
        app:layout_constraintTop_toBottomOf="@id/iv_about_app_profile"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="70dp"/>
    <TextView
        android:id="@+id/tv_logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/log_out"
        android:textSize="16sp"
        android:textColor="@color/red500"
        android:fontFamily="@font/poppins_semibold"
        app:layout_constraintStart_toEndOf="@id/iv_logout"
        app:layout_constraintTop_toTopOf="@id/iv_logout"
        app:layout_constraintBottom_toBottomOf="@id/iv_logout"
        android:layout_marginStart="20dp"/>



</androidx.constraintlayout.widget.ConstraintLayout>